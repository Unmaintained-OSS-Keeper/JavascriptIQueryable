<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tmpl.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    @RenderSection("header", false)
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>My MVC Application - JQueryLinq</h1>
            </div><br />
            <div id="menucontainer">
              <ul id="menu">
                <li>@Html.ActionLink("Home", "Index", "Home")</li> 
                <li>@Html.ActionLink("Client template", "Index", "Grid1")</li>     
                <li>@Html.ActionLink("Server template", "Index", "Grid2")</li>     
                <li>@Html.ActionLink("NoLinqEnable", "Index", "Grid3")</li>  
                <li>@Html.ActionLink("Mobile", "Index", "Mobile")</li>
                <li>@Html.ActionLink("About", "About", "Home")</li>
              </ul>
            </div>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
