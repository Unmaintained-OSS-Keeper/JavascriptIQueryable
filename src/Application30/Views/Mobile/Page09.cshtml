@using JQueryLinq
<!DOCTYPE html>

<html>
<head>
<title>Pagina7</title>
<meta name="viewport" content="initial-scale=2, maximum-scale=4">
<link href="../../JQueryMobile/jquery.mobile-1.0rc2.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="../../JQueryMobile/jquery-1.6.2.min.js"></script>
<script type="text/javascript" src="../../Scripts/jquery.tmpl.min.js"></script>
<script type="text/javascript" src="../../Scripts/jquery.unobtrusive-ajax.min.js" ></script>
<script type="text/javascript" src="../../Scripts/jquery.validate.min.js" ></script>
<script type="text/javascript" src="../../Scripts/jquery.validate.unobtrusive.min.js" ></script>
<script type="text/javascript" src="../../Scripts/knockout-2.0.0.js"></script>
<script type="text/javascript" src="../../Scripts/knockout.mapping.js"></script>
<script type="text/javascript" src="../../Scripts/json2.js"></script>
<script type="text/javascript" src="../../Scripts/zPageGrid1.js"></script>
<script type="text/javascript" src="../../Scripts/zPageGrid3.js"></script>
<script type="text/javascript" src="../../Scripts/Mobile/zPage09.js"></script>
<script type="text/javascript" src="../../JQueryMobile/jquery.mobile-1.0rc2.js"></script>
</head>
<body>

<!-- page -->
<div id="page1" data-role="page">
	<div data-role="header">
		<h1>Page Title 1a</h1>
	</div>
	<div data-role="content">	
		<p>Page content-1 goes here (@DateTime.Now).</p>
    <div>
    <table>    
    <tr>
      <td>City: </td>
      <td><input id="text1" type="text" /></td>
    </tr>  
    <tr>
      <td>Country: </td>
      <td><input id="text2" type="text" /></td>
    </tr> 
    </table>
    <br /><br />
    <a href="#page2" data-transition="fade">Find</a>
    <br /><br />
    </div>	
	</div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div>
</div>
<!-- /page --> 

<!-- page -->
<div id="page2" data-role="page" data-jresturl="GetData1" data-jautocompose="false" data-jenableko="true" data-jpagesize="10"
          data-jcontainer="c2" data-jinitialize="init2" data-jfselecting="selecting2" data-jfdatabound="databound2">
	<div data-role="header">
		<h1>Page Title-2</h1>
    <div data-role="controlgroup" data-type="horizontal">
      <a id="buttonR" data-role="button" data-jnavbaraction="refresh">Refresh</a>
      <a id="buttonF" data-role="button" data-jnavbaraction="pageF">MoveF</a>
      <a id="buttonP" data-role="button" data-jnavbaraction="pageP">MoveP</a>
      <a id="buttonN" data-role="button" data-jnavbaraction="pageN">MoveN</a>
      <a id="buttonL" data-role="button" data-jnavbaraction="pageL">MoveL</a>
    </div>    
	</div>
	<div data-role="content">	
		<p>Page content-2 goes here (@DateTime.Now).</p>
    <a data-rel="back" data-jbackcurrpage="true" data-direction="reverse">Back</a><br /> 		
    <div>
    <table id="jtable2">
    <thead>
    <tr>
      <td colspan="5"> &nbsp; </td>
      <td data-sort="CustomerID"> CustomerID </td>
      <td data-sort="CompanyName"> CompanyName </td>
      <td data-sort="ContactName"> ContactName </td>
      <td data-sort="City"> City </td>
      <td data-sort="Country"> Country </td>
      <td> Value1 </td>
      <td> Value2 </td>
      <td> Total </td>
    </tr>
    </thead>
    <tbody id="c2" data-bind="template: {name: 'temp2', foreach: $data}"></tbody>
    </table>
    </div>	
	</div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div>
</div>
<!-- /page -->

<!-- page -->
<div id="page3" data-role="page" data-jresturl="GetData1" data-jenableko="true" data-jcontainer="c3" data-jparam01="*City"
               data-jorderby="ContactName" data-jfselecting="selecting3" data-jfdatabound="databound3">
	<div data-role="header">
		<h1>Page Title-4</h1>
	</div>
	<div data-role="content">	
		<p>Page content-3 goes here (@DateTime.Now).</p>		
    <a data-rel="back" data-jbackcurrpage="true" data-direction="reverse">Back</a><br /> 		
    <div id="c3" data-bind="template: {name: 'tempdetail', foreach: $data}"></div> 
	</div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div>
</div>
<!-- /page -->

<!-- page -->
<div id="page4" data-role="page" data-jresturl="none" data-jenableko="true" data-jcontainer="c4"
                      data-jfselecting="selecting4" data-jfdatabound="databound4">
	<div data-role="header">
		<h1>Page Title-4</h1>
	</div>
	<div class="modifyfrm" data-role="content">	
		<p>Page content-4 goes here (@DateTime.Now).</p>		
    <a data-rel="back" data-jbackcurrpage="true" data-direction="reverse">Back</a><br />     
    <div id="c4" data-bind="template: {name: 'tempmodify1', foreach: $data}"></div> 
	</div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div>
</div>
<!-- /page -->

<!-- page -->
<div id="page5" data-role="page" data-jresturl="none" data-jenableko="true" data-jcontainer="c5"
                      data-jfselecting="selecting5" data-jfdatabound="databound5">
	<div data-role="header">
		<h1>Page Title-5</h1>
	</div>
	<div class="modifyfrm" data-role="content">	
		<p>Page content-5 goes here (@DateTime.Now).</p>		
    <a data-rel="back" data-jbackcurrpage="true" data-direction="reverse">Back</a><br />     
    <div id="c5" data-bind="template: {name: 'tempmodify2', foreach: $data}"></div> 
	</div>
	<div data-role="footer">
		<h4>Page Footer</h4>
	</div>
</div>
<!-- /page -->

<script id="temp2" type="text/html">
  <tr>
    <td><b data-jcustomaction="true">Custom</b></td>
    <td><b data-jdeleteaction="true">Delete</b></td>
    <td><a href="#page4" data-jsavecurritem="true">Edit-1</a></td>
    <td><a href="#page5" data-jsavecurritem="true">Edit-2</a></td>
    <td><a href="#page3" data-jsavecurritem="true">City</a></td>
    <td> ${CustomerID} </td>
    <td> ${CompanyName} </td>
    <td> ${ContactName} </td>
    <td> ${City} </td>
    <td> ${Country} </td>
    <td> ${Value1} </td>
    <td> ${Value2} </td>
    <td> ${Total} </td>
  </tr>
</script>

<script id="tempdetail" type="text/html">
  <div>
    <b>CustomerID: </b><span data-bind="text: CustomerID" /></span><br>
    <b>CompanyName: </b><span data-bind="text: CompanyName" /></span><br>
    <b>ContactName: </b><span data-bind="text: ContactName" /></span><br>
    <b>City: </b><span data-bind="text: City" /></span><br>
    <b>Country: </b><span data-bind="text: Country" /></span><br>
    <br>
  </div>
</script>

<script id="tempmodify1" type="text/html">
@Html.Partial("Page91")
</script>

<script id="tempmodify2" type="text/html">
@Html.Partial("Page92")
</script>

</body>
</html>
